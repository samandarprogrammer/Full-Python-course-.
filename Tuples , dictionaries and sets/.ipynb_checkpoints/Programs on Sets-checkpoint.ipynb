    "def Sum_unique(l):\n",
    "    s=set()\n",
    "    for i in l:\n",
    "        s.add(i)\n",
    "    sum=0\n",
    "    for i in s:\n",
    "        sum+=i\n",
    "    return sum\n",
    "l=[1,2,3,4,4,5,6,8,9,9,4]\n",
    "Sum_unique(l)"
   ]
  }
