    "def Print_K_freq_string(s,k):\n",
    "    words=s.split()\n",
    "    di={}\n",
    "    for w in words:\n",
    "        di[w]=di.get(w,0)+1\n",
    "    for w in di:\n",
    "        if di[w]==k:\n",
    "            print(w)\n",
    "s=input()\n",
    "k=int(input())\n",
    "Print_K_freq_string(s,k)"
   ]
  }
